<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.funguide.cc.movieticketmodule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TicketMainActivity" />
        <activity android:name=".activity.TicketProblemActivity" />
        <activity android:name=".activity.MyTicketActivity" />
        <activity android:name=".activity.AllFilmActivity" />
        <activity android:name=".activity.AllCinemaActivity" />
        <activity android:name=".activity.MyOderActivity" />
        <activity android:name=".activity.MyCouponActivity" />
        <activity android:name=".activity.CinemaSessionActivity"></activity>
        <activity android:name=".activity.FuzzySearchActivity"></activity>
    </application>

</manifest>